bir             DetIndef;
birkaç          DetIndef;
birkaç% tane    DetIndef;
birtakım        DetIndef;
birço^k         DetIndef;
bˈiraz           DetIndef;
bir% miktar     DetIndef;
bˈazı            DetIndef;
kimi            DetIndef;
bir% kısım      DetIndef;
bir% kesim      DetIndef;
bir% bölüm      DetIndef;
bu% kadar       DetIndef;
şu% kadar       DetIndef;
o% kadar        DetIndef;
okadar          DetIndef;
ne% kadar       DetIndef;
ço^k            DetIndef;
pek% ço^k       DetIndef;
pekço^k         DetIndef;
çoğu            DetIndef; ! no pronoun
az              DetIndef;
fazla           DetIndef;
hiçbir          DetIndefNeg;
hiç% bir        DetIndefNeg;
herhangi        DetIndef;
herhangi% bir   DetIndef;
başka           DetIndef;
bˈunca           DetIndef;
öyle            DetIndef;
böyle           DetIndef;
böylesi         DetIndef; ! no pronoun 
böylesine       DetIndef; ! no pronoun -- this is rather non-standard 
şöyle           DetIndef;
öyle% bir       DetIndef;
böyle% bir      DetIndef;
şöyle% bir      DetIndef;
! kaç           DetQIndef; ! we mark this as number
ne% kadar       DetIndefQ;
! definite_determiners_below_this_line#
iki             DetDef;     ! both
o               DetDef;
bu              DetDef;
şu              DetDef;
her             DetDef;    !no pronoun
her% bir        DetDef;
herbir          DetDef;
bütün           DetDef;
tüm             DetDef;
aynı            DetDef;
öte             DetDef;    ! somewhat colloquial
öteki           DetDef;    ! somewhat colloquial
beri            DetDef;    ! somewhat colloquial
beriki          DetDef;    ! somewhat colloquial
öbür            DetDef;
diğer           DetDef;
tek             DetDef;
hangi           DetDefQ;
